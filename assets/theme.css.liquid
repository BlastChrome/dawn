/**
* Convert PX to EM
*
* em-convert is a function of the gulp postcss-em package
*
**/
/**
* Example
**/
/**
* Media queries
**/
body {
  font-family: var(--font-family-2);
  color: var(--clr-smoke-2);
}

h1, h2, h3, h4, h5, h6 {
  font-family: var(--font-family-1);
  color: var(--clr-smoke-2);
  text-transform: uppercase;
  margin-bottom: 20px;
}

h1 {
  font-family: var(--font-family-1);
  font-size: var(--theme-font-1);
  color: var(--clr-smoke-2);
  line-height: 80px;
}

/**
* Convert PX to EM
*
* em-convert is a function of the gulp postcss-em package
*
**/
/**
* Example
**/
/**
* Media queries
**/
a.button {
  display: block;
  border: none;
  text-decoration: none;
  cursor: pointer;
  width: -moz-fit-content;
  width: fit-content;
  padding: 22px 40px;
  text-transform: uppercase;
  overflow: hidden;
  position: relative;
  max-width: 240px;
  display: flex;
  justify-content: center;
  font-family: var(--font-family-2-1);
  font-size: var(--theme-font-2);
  color: var(--clr-white);
  line-height: 24px;
}
a.button:after {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: var(--clr-white);
  transform: translateX(-105%);
  transition: transform 0.4s ease-in-out;
}
a.button:hover:after {
  transform: translateX(0);
}
a.button span {
  transition: all 0.3s;
  position: relative;
  z-index: 1;
}
a.button--primary {
  border: 2px solid var(--clr-red);
  background-color: var(--clr-red);
}
a.button--primary:hover {
  color: var(--clr-red);
}
a.button--secondary {
  border: 2px solid var(--clr-yellow);
  color: var(--clr-black);
  background-color: var(--clr-yellow);
}
a.button--secondary:hover {
  color: var(--clr-yellow);
}

